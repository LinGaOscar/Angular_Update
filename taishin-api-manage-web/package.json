{
  "name": "taishin-api-manage-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration es5",
    "build": "ng build --base-href=/taishin-api/",
    "build-local": "ng build --configuration=local",
    "build-base-href": "ng build --configuration=local --base-href=/taishin-api/",
    "build-prod": "ng build --prod --base-href=/taishin-api/ --named-chunks --output-hashing=none",
    "postbuild-prod": "cd build && ren default~auth-auth-module-ngfactory~auth-password-password-module-ngfactory~taishin-taishin-module-ngfactory.js RENAME_auth-module-ngfactory-password~taishin.js && ren default*bra-bra-module-ngfactory~brl-brl-module-ngfactory*staifSta-module-ngfactory~stl-stl-module-ngfactory.js RENAME_bra-module-ngfactory~brl~sta~stl.js",
    "build-prod-ldap": "ng build --configuration ldap --base-href=/taishin-api/frontend/ --named-chunks --output-hashing=none --output-path=frontend",
    "postbuild-prod-ldap": "cd frontend && ren default~auth-auth-module-ngfactory~auth-password-password-module-ngfactory~taishin-taishin-module-ngfactory.js RENAME_auth-module-ngfactory~password~taishin.js && ren default~bra-bra-module-ngfactory~brl-brl-module-ngfactory~sta-sta-module-ngfactory~stl-stl-module-ngfactory.js RENAME_bra-module-ngfactory~brl~sta~stl.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/material-moment-adapter": "^8.2.3",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@ngrx/effects": "^8.0.0",
    "@ngrx/store": "^8.0.0",
    "@ngx-translate/core": "^12.1.2",
    "@ngx-translate/http-loader": "^4.0.0",
    "@tbkc/datepicker": "^1.1.1",
    "@types/big.js": "^6.0.0",
    "big.js": "^6.0.2",
    "bigdecimal": "^0.6.1",
    "classlist.js": "^1.1.20150312",
    "crypto-js": "^4.0.0",
    "moment": "^2.29.4",
    "ng2-date-picker": "^8.0.0",
    "ngx-cookie-service": "^2.3.0",
    "rxjs": "^6.5.0",
